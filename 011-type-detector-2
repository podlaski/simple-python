# type detector (letter, digit or whitespace)

text = input('Add any string of characters (letter, digit or whitespace): ')

for x in text:
  if x.isalpha():
    char = 'letter'
  elif x.isdigit():
    char = 'digit'
  elif x.isspace():
    char = 'whitespace'
  else:
    char = 'unknown'
  print(x, '- ', char)
